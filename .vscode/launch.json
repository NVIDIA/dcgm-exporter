    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "cwd": "${workspaceRoot}",
            "program": "cmd/dcgm-exporter/main.go",
            "args": [
